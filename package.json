{
  "name": "npmclone",
  "version": "1.0.0",
  "description": "Fork this repo, then clone it to your computer",
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^8.5.0",
    "node-sass": "^4.9.0",
    "postcss-cli": "^5.0.0"
  },
  "scripts": {
    "sass": "node-sass source/css/style.scss source/css/style.css",
    "concat-css": "cat source/css/*.css > build/css/all.css",
    "autoprefix": "post.css input/*.css --use autoprefixer -d output/css/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hermansens/bloated.git"
  },
